{
    "name": "hyperf/resource-grpc",
    "description": "A api resource component for hyperf grpc server.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "resource",
        "grpc"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=8.1",
        "hyperf/grpc-server": "~3.0.0",
        "hyperf/resource": "~3.0.0",
        "hyperf/tappable": "~3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\ResourceGrpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\ResourceGrpc\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\ResourceGrpc\\ConfigProvider"
        }
    }
}
